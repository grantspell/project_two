{{!-- DISPLAY USERS CARDS --}}
<div>
    <h1>{{user.name}}'s Cards</h1>
    <h4>Username: <a href="/users/{{userId}}">@{{user.username}}</a></h4>
    <div>
    <input type="submit" value="Edit User" onclick="window.location=`/users/{{user._id}}/edit`;" />
    <input type="submit" value="Delete User" onclick="window.location=`/users/{{user._id}}/delete`;" />
    </div>
    <hr>
    <h3>{{user.name}}'s Printable Cards</h3>
    <br>
    {{#each user.userPrintCards}}
        <div>
            {{this.cardName}}
            <br>
            {{this.celebrant.firstName}}
        </div>
        <br>
    {{/each}}

    <h3>{{user.name}}'s eCards</h3>
    <br>
    {{#each user.userECards}}
        <div>
            {{this.cardName}}
            <br>
            {{this.celebrant.firstName}}
        </div>
        <br>
    {{/each}}

</div>